{
  "name": "questionnaire",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "normalize.css": "^8.0.1",
    "pug-plain-loader": "^1.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "pug": "^2.0.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "import/no-unresolved": "off",
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "state",
            "acc",
            "e"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "optionalDependencies": [
            "resources/test/unit/index.js"
          ]
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "_": false,
      "$": false,
      "localStorage": false,
      "Vue": true,
      "window": false,
      "document": false,
      "$trans": true,
      "socket": true,
      "ui": false,
      "Validator": false,
      "google": false,
      "lang": false
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "description": "Clean vue project",
  "main": "babel.config.js",
  "author": "Mikhail Kanavalov",
  "license": "ISC"
}
